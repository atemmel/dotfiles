alt + Return
	$TERMCMD

alt + d
	rofi -show run

alt + D
	~/.bin/docs

alt + y
	~/.bin/playclip

alt + m
	$TERMCMD -e ncmpcpp

alt + f
	$TERMCMD -e ~/.bin/spider

alt + p
	scrot

alt + P
	~/.bin/roms

alt + control + p
	flameshot gui

alt + control + h
	~/.bin/headers

alt + x
	~/.bin/shutdownmenu

alt + c
	"$BROWSER"

alt + control + e
	~/.bin/emoji

alt + n
	~/.bin/gonote-list

alt + N
	~/.bin/gonote-append

alt + s
	~/.bin/skyrim-launch

alt + S
	pkill -9 TESV.exe

alt + i
	i3lock-fancy
